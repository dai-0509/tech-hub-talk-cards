{
  "version": 2,
  "buildCommand": "npm run client:build",
  "outputDirectory": "dist/client",
  "functions": {
    "api/index.ts": {
      "includeFiles": "shared/**"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}